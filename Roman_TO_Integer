class Solution{

public:
    int romanToInt(string s ){
    int sum=0;
    map<char,int>m{{'I',1},{'V',5},{'X',10},{'L',50},{'C',100},{'D',500},{'M',1000}};

    for(int i=0;i<s.size();i++){
        int temp=m[s[i]];
        cout<<temp<<endl;
        if(i==s.size()-1||m[s[i+1]]<=m[s[i]]){
            sum=sum+temp;
        }else{

            sum=sum-temp;
        }
    }
    return sum;
    }
};
